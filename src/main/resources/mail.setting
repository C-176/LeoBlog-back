# 邮件服务器的SMTP地址，可选，默认为smtp.<发件人邮箱后缀>
host = smtp.163.com
# 邮件服务器的SMTP端口，可选，默认25
port = 25
# 发件人邮箱（必须正确，否则发送失败）
from = *
# 用户名（邮箱前缀，例如123.qq.com,取123），必须要设置成你自己使用邮箱的名称，否则会报错，权限认证失败  535错误
user = *
# 密码（注意，某些邮箱需要为SMTP服务单独设置授权码，详情查看相关帮助）
pass = *
# 使用SSL安全连接
sslEnable = false
#使用 STARTTLS安全连接，STARTTLS是对纯文本通信协议的扩展。
starttlsEnable = false
auth = true
